import sys
import os


def main() -> None:
    status = True

    while status:
        try:
            prompt = f"{os.getcwd()}$> "
            line = input(prompt).strip()
        except (EOFError, KeyboardInterrupt):
            # Graceful exit on Ctrl-D / Ctrl-C
            print()
            break

        if not line:
            continue

        parts = line.split()
        if parts[0] != "milo":
            continue

        match (cmd:= parts[1]):
            case "execute" | "exec":
                pass

            case "close" | "cl":
                status = False


if __name__ == "__main__":
    main()